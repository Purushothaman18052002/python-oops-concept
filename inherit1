class vechicle:
    def info (self)
        print("this is vechicle")
class car(vechicle):
    def car_info (self,name):
        print("car name is:",name)
class truck(vechicle):
    def truck_info(self,name):
        print("truck name is:",name)
obj1=car()
obj1.info()
obj1.car_info('BMW')
obj2=truck()
obj2.info()
obj2.truck_info('FORD')
   